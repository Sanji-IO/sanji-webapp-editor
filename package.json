{
  "name": "sanji-webapp-editor",
  "author": "Zack Yang <zack9433@gmail.com> (https://github.com/zack9433)",
  "version": "1.0.0",
  "description": "MOXA Sanji Webapp Editor",
  "config": {
    "ghooks": {
      "pre-commit": ""
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "NODE_ENV=test karma start",
    "test:once": "karma start",
    "start": "NODE_ENV=development BLUEBIRD_WARNINGS=0 nodemon ./server/boot.js | bunyan -o short",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prepublish": "npm run build",
    "postpublish": "publish-latest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-webapp-editor.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/Sanji-IO/sanji-webapp-editor",
  "devDependencies": {
    "angular-mocks": "~1.4.8",
    "autoprefixer-loader": "~3.1.0",
    "babel-core": "~5.8.34",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~5.3.3",
    "babel-runtime": "~5.8.34",
    "commitizen": "~2.4.6",
    "css-loader": "~0.23.0",
    "cz-conventional-changelog": "~1.1.5",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "expose-loader": "~0.7.1",
    "extract-text-webpack-plugin": "~0.9.1",
    "file-loader": "~0.8.5",
    "ghooks": "~1.0.1",
    "html-webpack-plugin": "~1.7.0",
    "isparta-loader": "~2.0.0",
    "istanbul": "~0.4.1",
    "json-loader": "~0.5.4",
    "karma": "~0.13.15",
    "karma-chai-plugins": "~0.6.1",
    "karma-coverage": "~0.5.2",
    "karma-es6-shim": "~0.2.0",
    "karma-mocha": "~0.2.1",
    "karma-mocha-reporter": "~1.1.3",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "ng-annotate-loader": "~0.0.10",
    "ng-cache-loader": "~0.0.14",
    "node-bourbon": "~4.2.3",
    "node-sass": "~3.4.2",
    "nodemon": "~1.8.1",
    "phantomjs": "~1.9.19",
    "publish-latest": "~1.1.2",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.1.2",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0",
    "webpack-notifier": "~1.2.1"
  },
  "dependencies": {
    "angular": "~1.4.8",
    "angular-drag-and-drop-lists": "~1.3.2",
    "angular-material": "~1.0.0",
    "angular-material-icons": "~0.6.0",
    "angular-sanji-window": "Sanji-IO/angular-sanji-window#webpack-sanji-window",
    "bcrypt": "~0.8.5",
