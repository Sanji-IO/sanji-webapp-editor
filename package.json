    "jsonwebtoken": "~5.4.1",
    "lodash": "~3.10.1",
    "method-override": "~2.3.5",
    "morgan": "~1.6.1",
    "multer": "~1.1.0",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "sanji-core-ui": "~1.10.1",
    "semver": "~5.1.0",
